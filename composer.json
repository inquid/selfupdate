{
    "name": "inquid/yii-cd-ci",
    "description": "Tools for Yii to use Continuous Delivery and Continuous Integration",
    "keywords": ["yii2", "selfupdate", "update", "deployment", "continuous", "integration", "VCS", "GIT","Inquid"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii2tech/selfupdate/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/yii2tech/selfupdate/wiki",
        "source": "https://github.com/yii2tech/selfupdate"
    },
    "authors": [
        {
            "name": "Inquid SAS de CV",
            "email": "contact@inquid.co"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"inquid\\yii-cd-ci\\": "src"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
